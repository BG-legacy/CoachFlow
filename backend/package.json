{
  "name": "coachflow-backend",
  "version": "1.0.0",
  "description": "CoachFlow - Fitness Coaching Platform Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "start:staging": "NODE_ENV=staging node src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "test": "NODE_ENV=test jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "config:validate": "node -e \"require('./src/common/config')\"",
    "config:init": "cp .env.template .env && echo 'âœ… Created .env file. Please edit it with your configuration.'",
    "secrets:generate": "node scripts/generate-secrets.js",
    "verify:google": "node scripts/verify-google-oauth.js",
    "test:google": "./scripts/test-google-oauth-endpoints.sh"
  },
  "keywords": [
    "fitness",
    "coaching",
    "api",
    "express"
  ],
  "author": "CoachFlow Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.0.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
